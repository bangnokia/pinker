{
  "name": "bangnokia/psycho",
  "type": "library",
  "description": "The psycho library",
  "license": "MIT",
  "require": {
    "php": "^7.3",
    "psy/psysh": "^0.10",
    "laravel/tinker": "^2.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/process": "^5.2"
  },
  "autoload": {
    "psr-4": {
      "BangNokia\\Psycho\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BangNokia\\Psycho\\Tests\\": "tests/"
    }
  },
  "config": {
    "platform-check": false
  }
}
