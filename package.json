{
  "name": "pinker",
  "productName": "Pinker",
  "version": "1.0.0",
  "description": "Pinker gonna tinker you",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build-laravel": "cd laravel && php artisan migrate:fresh --seed && php artisan config:clear && npm run production && cd ../",
    "build": "npm run build-laravel && electron-packager . --icon=assets/icon.icns --ignore=laravel/node_module --out=builds --overwrite"
  },
  "keywords": [],
  "author": "bangnokia",
  "license": "MIT",
  "devDependencies": {
    "electron": "^11.2.1",
    "electron-packager": "^15.2.0",
    "electron-reloader": "^1.2.0"
  },
  "dependencies": {
    "fix-path": "^3.0.0",
    "php-server-manager": "^1.3.3"
  }
}
